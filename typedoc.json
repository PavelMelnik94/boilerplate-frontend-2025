{
  "entryPoints": ["src"],
  "entryPointStrategy": "expand",
  "out": "docs/api",
  "plugin": ["typedoc-plugin-markdown"],
  "exclude": ["**/*.test.ts", "**/*.spec.ts", "**/node_modules/**"],
  "excludePrivate": true,
  "excludeProtected": true,
  "excludeExternals": true,
  "includeVersion": true,
  "gitRevision": "master",
  "hideGenerator": true,
  "readme": "none",
  "theme": "markdown",
  "categorizeByGroup": true,
  "categoryOrder": ["Components", "Hooks", "Utils", "Types", "*"],
  "sort": ["source-order"]
}
